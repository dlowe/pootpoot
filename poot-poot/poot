#!/usr/bin/env python
"""fetch metadata describing a single random interpretation"""

import random

INTERPRETATIONS = [
    {'id': 0, 'type': "javascript"},
    {'id': 1, 'type': "text"},
    {'id': 2, 'type': "image"},
]

def main():
    """mock interpretation selector"""

    i = random.choice(INTERPRETATIONS)
    print """Content-Type: text/plain

{
 "id": %(id)d,
 "type": "%(type)s",
 "location": "/i/%(id)d"
}
""" % i

if __name__ == "__main__":
    main()
