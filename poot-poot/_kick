#!/usr/bin/env python

## app engine
from google.appengine.ext import db
from google.appengine.ext import webapp
from google.appengine.ext.webapp.util import run_wsgi_app

## this app
from pypoot import interpretation

class Kick(webapp.RequestHandler):
    def get(self):
        query = db.GqlQuery("SELECT __key__ FROM Interpretation")
        keys = query.fetch(1000)
        for key in keys:
            i = db.get(key)
            i.owner_baton = interpretation._new_owner_baton()
            i.put()

        self.response.out.write("ok")

APPLICATION = webapp.WSGIApplication([('/_kick', Kick)], debug=True)

def main():
    run_wsgi_app(APPLICATION)

if __name__ == "__main__":
    main()
